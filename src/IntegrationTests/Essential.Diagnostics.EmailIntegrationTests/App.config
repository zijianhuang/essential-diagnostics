<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.diagnostics>

    <trace autoflush="true" indentsize="4" useGlobalLock="false">
      <listeners>
        <add name="emailTraceListener" initializeData="user2@example.com" type="Essential.Diagnostics.EmailTraceListener, Essential.Diagnostics.EmailTraceListener" maxConnections="8"/>

      </listeners>
    </trace>

  </system.diagnostics>

  <system.net>
    <mailSettings>
      <smtp deliveryMethod="Network" from="user1@example.com">
        <network host="localhost" port="25" defaultCredentials="true"/>
      </smtp>
    </mailSettings>
  </system.net>

<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/></startup></configuration>
